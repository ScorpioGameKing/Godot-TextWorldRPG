; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="TextworldRPG"
run/main_scene="res://Scenes/Menu/main_menu.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://Sprites/icon.svg"

[autoload]

InitData="*res://Scripts/AutoLoads/Initialization.gd"
EventManager="*res://Scripts/AutoLoads/EventManager.gd"
WorldManager="*res://Scripts/AutoLoads/WorldManager.gd"
EditorManager="*res://Scripts/AutoLoads/editor_manager.gd"

[display]

window/size/resizable=false

[editor_plugins]

enabled=PackedStringArray("res://addons/Asset_Drawer/plugin.cfg", "res://addons/Todo_Manager/plugin.cfg", "res://addons/cyberassets/plugin.cfg", "res://addons/project_time_tracker/plugin.cfg", "res://addons/ridiculous_coding/plugin.cfg", "res://addons/script-ide/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
